{
  "$schema": "http://json-schema.org/2020-12/schema#",
  "$id": "https://conuti.de/bo4e/schemas/v1/com/Angebotsvariante",
  "type": "object",
  "properties": {
    "angebotsstatus": {
      "$ref": "../enum/Angebotsstatus.schema.json"
    },
    "beschreibung": {
      "type": "string"
    },
    "erstelldatum": {
      "type": "string",
      "format": "date-time"
    },
    "bindefrist": {
      "type": "string",
      "format": "date-time"
    },
    "gesamtmenge": {
      "$ref": "../com/Menge.schema.json"
    },
    "gesamtkosten": {
      "$ref": "../com/Betrag.schema.json"
    },
    "teile": {
      "type": "array",
      "items": {
        "$ref": "../com/Angebotsteil.schema.json"
      }
    }
  }
}
