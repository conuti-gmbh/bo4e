{
  "$schema": "http://json-schema.org/2020-12/schema#",
  "$id": "https://conuti.de/bo4e/schemas/v1/bo/Preisblatt",
  "type": "object",
  "properties": {
    "boTyp": {
      "type": "string"
    },
    "versionStruktur": {
      "type": "string"
    },
    "bezeichnung": {
      "type": "string"
    },
    "gueltigkeit": {
      "$ref": "../com/Zeitraum.schema.json"
    },
    "preisstatus": {
      "$ref": "../enum/Preisstatus.schema.json"
    },
    "sparte": {
      "$ref": "../enum/Sparte.schema.json"
    },
    "bilanzierungsdatum": {
      "type": "string"
    },
    "regelzone": {
      "type": "string"
    },
    "preispositionen": {
      "type": "array",
      "items": {
        "$ref": "../com/Preisposition.schema.json"
      }
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
